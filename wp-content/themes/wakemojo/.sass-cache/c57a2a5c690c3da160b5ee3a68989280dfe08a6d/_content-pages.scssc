:@template"²%/*--------------------------------------------------------------
10.04 Content - Pages
--------------------------------------------------------------*/
.page-links {
	clear: both;
	margin: 0 0 1.5em;
}
h2, h3{
	color: #032454;
	font-size: 20px !important;
	text-transform: uppercase;
}
h2{
	font-family: 'Oswald', sans-serif !important;
}
h3{
	font-family: Helvetica, sans-serif !important;
}
.logo{
	text-align: center;
	margin: -45px auto 0 auto;
	position: relative;
	z-index: 9999;
	width: 263px;
}
.hero{
	margin: -92px auto 0 auto;
	text-align: center;
	position: relative;
	z-index: 9;
	overflow: hidden;
	img{
		width: 100%;
	}
	&.single{
		background: url(/wp-content/themes/wakemojo/img/boards-hero.jpg) no-repeat top center;
		background-size: cover;
		padding: 14% 0;
		top: 0;
		left: 0;
	}
	.wrap{
		position: relative;
		overflow: hidden;
		width: 59%;
		margin: 0 auto;
		font-family: Helvetica, sans-serif;
		font-size: 12px;
		text-align: left;
		.board-image{
			float: left;
			width: 60%;
			text-align: center;
		}
		.sidebar{
			color: #032454;
			float: right;
			width: 37%;
		}
		.sidebar-top{
			background: url(/wp-content/themes/wakemojo/img/sidebar-bg.png);
			padding: 0 30px 20px 30px;
		}
		.brand-name{
			font-family: 'Oswald', sans-serif;
			font-size: 32px;
			text-transform: uppercase;
			padding-top: 15px;
			a, a:visited{
				color: #032454;
				text-decoration: none;
			}
		}
		.summary{
			padding-bottom: 20px;
		}
		.board-title{
			font-family: 'Oswald', sans-serif;
			font-size: 20px;
			text-transform: uppercase;
			padding-bottom: 20px;
		}
		.team-size{
			padding-bottom: 20px;
		}
		.team-rider, .fins-title, .shaped-by-title, .cf-title{
			font-family: Helvetica, sans-serif;
			text-transform: uppercase;
			font-weight: bold;
			font-size: 14px;
		}
		.shaped-by, .construction-features{
			padding-bottom: 20px;
		}
		img{
			max-width: 479px;
			max-height: 615px;
			width: auto;
		}
		.sidebar-bottom{
			padding: 20px 30px;
			background: url(/wp-content/themes/wakemojo/img/link-bg.png);
			margin-top: 10px;
			a{
				color: #032454;
				text-transform: uppercase;
				font-family: Helvetica, sans-serif;
				text-decoration: none;
				font-size: 13px;
				font-weight: 600;
			}
		}
	}
}
.green-bar{
	background: #77CA93;
	margin-top: -7px;
	width: 100%;
	.wrap{
		width: 1060px;
		margin: 0 auto;
		overflow: hidden;
		padding: 15px 0;
	}
	.reference{
		float: left;
		font-family: Helvetica, sans-serif;
		font-size: 20px;
		font-weight: 400;
		letter-spacing: 2px;
		color: #fff;
		text-transform: uppercase;
		padding-top: 8px;
	}
	.blog-link{
		float: right;
		overflow: hidden;
	}
	.arrow{
		float: left;
	}
	.blog-link-text{
		float: right;
		padding: 7px 0 0 15px;
	}
	a, a:visited{
		font-family: 'Raleway', sans-serif;
		color: #032454;
		font-size: 18px;
	}
}
.homepage-categories{
	background: url(/wp-content/themes/wakemojo/img/gate-bg.png);
	width: 100%;
	overflow: hidden;
	.wrap{
		width: 1060px;
		margin: 0 auto;
		overflow: hidden;
		padding: 40px 0;
	}
	.featured-box{
		float: left;
		width: 300px;
		.text{
			background: #DFFFFF;
			margin-top: -7px;
			padding: 0 42px 20px 42px;
			font-family: Helvetica, sans-serif;
			color: #032454;
			font-size: 12px;
			h2{
				font-family: 'Oswald', sans-serif;
				color: #032454;
				font-size: 20px;
				font-weight: 400;
				text-transform: uppercase;
			}
			p{
				min-height: 75px;
			}
			.view-all{
				text-align: center;
				color: #fff;
				text-transform: uppercase;
				background: #032454;
				font-family: 'Oswald', sans-serif;
				text-decoration: none;
				width: 100px;
				margin: 0 auto;
			}
		}
	}
	.boards, .boats{
		margin-right: 80px;
	}
}
.cta{
	padding: 65px 0 40px;
	.wrap{
		background: url(/wp-content/themes/wakemojo/img/trans-blue-bg.png);
		margin: 0 auto;
		width: 1080px;
		padding: 30px 335px;
		h2{
			font-family: 'Oswald', sans-serif;
			font-size: 40px;
			text-transform: uppercase;
			text-align: center;
			a{
				color: #032454;
				text-decoration: none;
			}
		}
	}
}
.contact-us{
	background: url(/wp-content/themes/wakemojo/img/trans-blue-bg.png);
	margin: 0 auto 80px auto;
	width: 375px;
	text-align: center;
	padding: 18px 0;
	a{
		color: #032454;
		text-decoration: none;
		font-size: 13px;
		text-transform: uppercase;
		font-weight: bold;
	}
}
.home .cta{
	background: url(/wp-content/themes/wakemojo/img/waves-bg.jpg) no-repeat center fixed;
	background-size: cover;
	padding: 65px 0;
}
.featured-posts{
	background: #99C3FF;
	width: 100%;
	overflow: hidden;
	.wrap{
		margin: 0 auto;
		width: 1180px;
		overflow: hidden;
		padding: 35px 0;
		font-family: Helvetica, sans-serif;
		color: #032454;
		font-size: 14px;
		h2{
			text-align: center;
			text-transform: uppercase;
			color: #032454;
			font-family: 'Oswald', sans-serif;
			font-size: 20px;
			padding-bottom: 20px;
		}
		.border{
			padding-bottom: 30px;
		}
		.title{
			font-weight: bold;
			font-size: 20px;
			a{
				color: #fff;
				text-decoration: none;
			}
		}
		.read-more{
			a{
				color: #032454;
				text-decoration: none;
			}
			background: #77CA93;
			padding: 5px 0;
			width: 88px;
			text-align: center;
			font-size: 11px;
			text-transform: uppercase;
		}
	}
	.featured-post{
		float: left;
		width: 230px;
		&.id-0, &.id-1, &.id-2{
			margin-right: 86px;
		}
	}
}
.page-title{
	font-family: 'Oswald', sans-serif;
	color: #fff;
	text-align: center;
	text-transform: uppercase;
	font-size: 20px;
}
.search .inner-wrap{
	padding: 30px !important;
}
.brands-loop{
	width: 100%;
	background: url(/wp-content/themes/wakemojo/img/gate-bg.png);
	overflow: hidden;
	.wrap{
		margin: 0 auto;
		width: 1316px;
		background: url(/wp-content/themes/wakemojo/img/blue-mist.png) repeat-y center;
		overflow: hidden;
	}
	.inner-wrap{
		margin: 0 auto;
		width: 1080px;
		overflow: hidden;
		background: #fff;
		padding: 30px 0 0 30px;
		&.review, &.blog{
			padding: 30px 150px;
			h2{
				font-family: 'Oswald', sans-serif;
				color: #032454;
				font-size: 20px;
				text-transform: uppercase;
			}
			h3, h3 a{
				color: #032454;
				font-size: 20px;
				font-family: Helvetica, sans-serif;
				text-transform: uppercase;
				font-weight: 600;
				text-decoration: none;
			}
		}
		&.blog{
			.post-container{
				overflow: hidden;
			}
			.image{
				float: left;
			}
			.content{
				float: right;
				width: 400px;
			}
			h3{
				color: #8FB9FF;
			}
		}
		&.page{
			font-family: 'Raleway', sans-serif;
			font-weight: 500;
			color: #8FB9FF;
		}
	}
	.brand-container{
		float: left;
		width: 322px;
		min-height: 262px;
		margin: 0 28px 28px 0;
		text-align: center;
		border: 1px solid #809FCB;
		p{
			font-family: 'Oswald', sans-serif;
			color: #032454;
			text-transform: uppercase;
			text-decoration: none;
		}
	}
	.view-all-boards{
		display: block;
		background: #032454;
		font-family: 'Oswald', sans-serif;
		color: #fff;
		text-transform: uppercase;
		font-size: 12px;
		width: 90px;
		text-align: center;
		margin: 0 auto;
		text-decoration: none;
	}
}
#bottom-container{
	background: url(/wp-content/themes/wakemojo/img/waves-bg.jpg) no-repeat center fixed;
	background-size: cover;
}
.board-brand-title{
	color: #032454;
	font-family: 'Oswald', sans-serif;
	font-size: 20px;
	text-transform: uppercase;
}
.quote{
	font-family: Helvetica, sans-serif;
	font-size: 20px;
	text-transform: uppercase;
	color: #032454;
	font-weight: 600;
	letter-spacing: 1px;
}
.brand-blurb{
	padding: 30px 30px 30px 0;
	color: #8FB9FF;
	font-family: 'Raleway', sans-serif;
	font-size: 18px;
	font-weight: 500;
}
.brand-link{
	font-family: Helvetica, sans-serif;
	color: #77C089;
	font-size: 14px;
	text-transform: uppercase;
	text-decoration: underline;
	a, a:visited{
		color: #77C089;
	}
}
.boards-loop-title{
	background: url(/wp-content/themes/wakemojo/img/loop-title-bg.png) no-repeat center;
	font-family: Helvetica, sans-serif;
	color: #032454;
	font-size: 20px;
	text-transform: uppercase;
	text-align: center;
	padding: 20px 0;
}
.brand-image.board{
	max-width: 157px;
}
.bottom-summary{
	width: 780px;
	margin: 0 auto;
	font-family: 'Raleway', sans-serif;
	font-weight: 500;
	color: #8FB9FF;
	font-size: 20px;
	padding: 40px 0;
}
.rating-title{
	float: left;
}
.rating{
	float: right;
}
.rating-container {
	overflow: hidden;
	width: 420px;
}
.expand {
	padding: 50px 0;
	text-align: center;
	cursor: pointer;
	width: 290px;
	margin: 0 auto;
	h3{
		padding-bottom: 20px;
	}
}
.review-content{
	display: none;
}
.fancybox{
	float: left;
	margin: 0 45px 30px 0;
}
.sorting-data{
	display: none
}
.sort-buttons, .filter-buttons{
	overflow: hidden;
	padding-bottom: 30px;
	position: relative;
	width: 100%;
	opacity: 0;
	transition: all .2s;
	-webkit-transition: all .2s;
	-moz-transition: all .2s;
	-o-transition: all .2s;
	-ms-transition: all .2s;
	h2, div{
		float: left;
		margin-right: 15px;
	}
	div{
		padding-top: 5px;
		cursor: pointer;
	}
	&.loaded{
		opacity: 1;
	}
}
.featured-image{
	float: left;
	padding-right: 20px;
}
.cotnent{
	float: right;
}
.option-set{
	display: none;
	float: none !important;
	position: absolute;
	top: 40%;
	&.style{
		left: 9%;
	}
	&.skill-level{
		left: 14%;
	}
	&.weight-range{
		left: 22%;
	}
}
.board-container{
	transition: all .2s;
	-webkit-transition: all .2s;
	-moz-transition: all .2s;
	-ms-transition: all .2s;
}
.sort-data{
	display: none;
}
.boat-sort-buttons{
	padding-bottom: 40px;
}
.gform_wrapper ul.gfield_checkbox li input[type=checkbox], .inner-wrap .gform_wrapper ul.gfield_radio li input[type=radio]{
	width: 18px !important;
}
.board-container.brands .brand-container img{
	max-height: 180px;
}






















